{
  "presets": [
    "react-native"
  ],
  "plugins": [
    "transform-export-extensions",
    "transform-object-rest-spread",
    "transform-class-properties",
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./app"],
        "extensions": [".js", ".ios.js", ".android.js"],
      }
    ]
  ]
}
